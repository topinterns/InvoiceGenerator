<?xml version="1.0" encoding="UTF-8"?>
<queue-entries>
<!--Change the refresh rate of the default queue to 1/s-->
  <queue>
    <name>taskQueueToReadTextFileAndCreateStaticMap</name>
    <rate>10/s</rate>
    <max-concurrent-requests>15</max-concurrent-requests>
  </queue>

	<queue>
    <name>constructDelimitFileOfGen</name>
    <rate>10/s</rate>
  </queue>

   <queue>
    <name>generateBulkPdf</name>
    <rate>3/s</rate>
  </queue>
  
  <queue>
    <name>generatePdfAndPushToCloudStorage</name>
    <rate>40/s</rate>
    <bucket-size>100</bucket-size>
    <max-concurrent-requests>30</max-concurrent-requests>
    <retry-parameters>
      <task-retry-limit>7</task-retry-limit>
      <task-age-limit>2d</task-age-limit>
    </retry-parameters>
  </queue>
  
</queue-entries>






